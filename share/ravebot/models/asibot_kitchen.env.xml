<Environment>
    <physicsengine type="ode">
        <odeproperties>
            <friction>0.01</friction>
            <gravity>0 0 -9.81</gravity>
        </odeproperties>
    </physicsengine>
    <!--camtrans>2.0 3.2 2.8</camtrans>

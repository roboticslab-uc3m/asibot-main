#!/usr/bin/env python

from distutils.core import setup
from os.path import normpath # see 320d03f

moduleName = 'AsibotPy'

setup(name = moduleName,
        version = '0.0.1',
        description = 'Application launcher for Asibot\'s web interface',
        author = 'Juan G. Victores',
        url = 'https://github.com/roboticslab-uc3m/asibot-main/',
        package_dir = {'': normpath('${CMAKE_SOURCE_DIR}/share/webInterface/user/')},
        py_modules = [moduleName],
    )

<!-- wheelchairLW.kinbody.xml -->

<KinBody name="wheelchairLWKinbody">
    <Body name="chair">
        <Geom type="box">
            <Render>./ivs/wheelchair.iv</Render>
            <Extents>0.2 0.5 .7</Extents>
            <Translation>0.2 0.5 0.1</Translation>
            <transparency>0.5</transparency>
            <diffuseColor>0 0 1</diffuseColor>
        </Geom>
    </Body>
    <Body name="chairDocking">
        <translation>-0.3 -0.05 -0.054</translation>
        <Geom type="box">
            <!--Render>./ivs/dstation.iv</Render-->
            <Extents>0.01 0.01 0.01</Extents>            
        </Geom>
    </Body>
    <Joint name="fake" type="hinge" enable="false">
        <body>chair</body>
        <body>chairDocking</body>
        <limits>0 0</limits>
    </Joint>
</KinBody>

